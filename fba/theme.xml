<theme>
	<formatVersion>4</formatVersion>
	<include>./../theme.xml</include>

	<view name="system">
		<image name="logo">
			<path>./_inc/background.jpg</path>
		</image>
	</view>
	
	<view name="basic, detailed, video">
		<image name="logo">
			<path>./_inc/system.svg</path>
		</image>
	</view>
	
	<view name="detailed, video">		
		<textlist name="gamelist">	
			<size>0.2083 0.3842</size>
		</textlist>		
			 		
   		<image name="bezel" extra="true">
			<path>./_inc/bezel_detailed.png</path>
		</image>
		
	</view>

	<feature supported="video">
      <view name="video">
		<video name="md_video">
			<rotation>0</rotation>
		</video>
      </view>
	</feature>
	
</theme>